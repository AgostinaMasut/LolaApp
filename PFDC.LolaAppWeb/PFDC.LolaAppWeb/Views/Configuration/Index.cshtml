@model Tuple<PFDC.LolaAppWeb.Models.Stretcher, PFDC.LolaAppWeb.Models.Machine,PFDC.LolaAppWeb.Models.Branch, PFDC.LolaAppWeb.Models.Treatment>

@{
	ViewBag.Title = "Configuración";
}

<h3>Configuración</h3>

<div id="accordion" role="tablist">
	<div class="card">
		<div class="card-header" role="tab" id="headingOne">
			<h5 class="mb-0">
				<button data-toggle="collapse" href="#collapseOne" aria-expanded="false" aria-controls="collapseOne" class="btn collapsed" style="background-color: #E99497">
					Camillas
				</button>
			</h5>
		</div>

		<div id="collapseOne" class="collapse" role="tabpanel" aria-labelledby="headingOne" style="">
			<div class="card-body col-md-4">
				@Html.Partial("_PartialStretchers", Model.Item1)
			</div>
		</div>
	</div>
	<div class="card">
		<div class="card-header" role="tab" id="headingTwo">
			<h5 class="mb-0">
				<button class="btn collapsed" data-toggle="collapse" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo" style="background-color: #E99497">
					Maquinas
				</button>
			</h5>
		</div>
		<div id="collapseTwo" class="collapse" role="tabpanel" aria-labelledby="headingTwo" style="">
			<div class="card-body">
				@Html.Partial("_PartialMachine", Model.Item2)
			</div>
		</div>
	</div>
	
	<div class="card">
		<div class="card-header" role="tab" id="headingThree">
			<h5 class="mb-0">
				<button class="btn collapsed" data-toggle="collapse" href="#collapseThree" aria-expanded="false" aria-controls="collapseThree" style="background-color: #E99497">
					Tratamientos
				</button>
			</h5>
		</div>
		<div id="collapseThree" class="collapse" role="tabpanel" aria-labelledby="headingThree" style="">
			<div class="card-body">
				@Html.Partial("_PartialTreatments", Model.Item4)
			</div>
		</div>
	</div>
	<div class="card">
		<div class="card-header" role="tab" id="headingFour">
			<h5 class="mb-0">
				<button class="btn collapsed" data-toggle="collapse" href="#collapseFour" aria-expanded="false" aria-controls="collapseFour" style="background-color: #E99497">
					Sucursales
				</button>
			</h5>
		</div>
		<div id="collapseFour" class="collapse" role="tabpanel" aria-labelledby="headingFour" style="">
			<div class="card-body">
				@Html.Partial("_PartialBranch", Model.Item3)
				
			</div>
		</div>
	</div>
</div>




