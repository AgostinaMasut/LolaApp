@model IEnumerable<LolaWebApp_Base.WebUi.Models.UserViewModel>

@{
    ViewBag.Title = "Usuarios";
}

<h2>Usuarios</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
